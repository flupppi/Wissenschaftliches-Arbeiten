@article{Schw:83,
	author = {Schwartz, Jacob T. and Sharir, Micha},
	title = {On the piano movers' problem I. The case of a two-dimensional rigid polygonal body moving amidst polygonal barriers},
	journal = {Communications on Pure and Applied Mathematics},
	volume = {36},
	number = {3},
	pages = {345-398},
	doi = {10.1002/cpa.3160360305},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cpa.3160360305%7D},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/cpa.3160360305%7D},
	abstract = {Abstract We present an algorithm that solves a two-dimensional case of the following problem which arises in robotics: Given a body B, and a region bounded by a collection of ?walls?, either find a continuous motion connecting two given positions and orientations of B during which B avoids collision with the walls, or else establish that no such motion exists. The algorithm is polynomial in the number of walls (O(n5) if n is the number of walls), but for typical wall configurations can run more efficiently. It is somewhat related to a technique outlined by Reif.},
	year = {1983}
}
